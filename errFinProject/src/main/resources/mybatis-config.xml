		<typeAlias type="com.kh.efp.member.model.vo.Member" alias = "Member"/>
		<typeAlias type="com.kh.efp.band.model.vo.Scehdule" alias = "Scehdule"/>
		<typeAlias type="com.kh.efp.band.model.vo.Band" alias = "Band"/>
