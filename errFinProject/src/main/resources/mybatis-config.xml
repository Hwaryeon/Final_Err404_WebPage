	</typeAliases>

	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/scehdule-mapper.xml"/>
		<mapper resource="mappers/search-mapper.xml"/>
		<mapper resource="mappers/member_band-mapper.xml"/>
