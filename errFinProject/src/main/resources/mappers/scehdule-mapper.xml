<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Scehdule">
	<resultMap type="Scehdule" id="ScehduleResultSet">
		<id property="did" column="DID" />
		<result property="bid" column="BID" />
		<result property="mid" column="MID" />
		<result property="sDate" column="SDATE" />
		<result property="eDate" column="EDATE" />
		<result property="title" column="TITLE" />
	</resultMap>

	<!-- 일정 등록용 메소드 -->
	<insert id="insertScehdule" parameterType="Scehdule">
		INSERT INTO SCEHDULE
		VALUES(SEQ_SCEHDULE.NEXTVAL, NULL, NULL, #{sDate}, #{eDate}, #{title})
	</insert>
	
	<!-- 일정 리스트 조회용 메소드 -->
	<select id="scehduleList" resultMap="ScehduleResultSet" parameterType="Scehdule">
		SELECT *
		FROM SCEHDULE
	</select>

</mapper>


















